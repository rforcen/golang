// bell 

const f0=404, f1=400;

func bp(f,x,y,z) ➡ ~f exp(1-t y) z;

func bell(f) ➡ 
 bp(f, 0.56,    0.075,  1)   +
 bp(f, 0.56+1,  0.9,    0.67)+
 bp(f, 0.92,    0.65,   1.35)+
 bp(f, 0.92+1.7,0.55,   1.8) +
 bp(f, 1.7,     0.35,   1.67)+
 bp(f, 1.19,    0.325,  2.67)+
 bp(f, 2,       0.25,   1.46)+
 bp(f, 2.74,    0.2,    1.33)+
 bp(f, 3,       0.15,   1.33)+
 bp(f, 3.75,    0.1,    0.75)+
 bp(f, 4.07,    0.075,  1.33);

bell(f0);
bell(f1);